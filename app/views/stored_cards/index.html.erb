<h1>Listing stored_cards</h1>

<table>
  <thead>
    <tr>
      <th>Creditcard</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stored_cards.each do |stored_card| %>
      <tr>
        <td><%= stored_card.creditcard_id %></td>
        <td><%= stored_card.customer_id %></td>
        <td><%= link_to 'Show', stored_card %></td>
        <td><%= link_to 'Edit', edit_stored_card_path(stored_card) %></td>
        <td><%= link_to 'Destroy', stored_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stored card', new_stored_card_path %>
