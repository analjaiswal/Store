<%= simple_form_for(@cart) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :customer_id %>
    <%= f.input :saname %>
    <%= f.input :creditcard_id %>
    <%= f.input :tstatus %>
    <%= f.input :tdate %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
