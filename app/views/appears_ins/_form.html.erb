<%= simple_form_for(@appears_in) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :cart_id %>
    <%= f.input :product_id %>
    <%= f.input :quantity %>
    <%= f.input :pricesold %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
